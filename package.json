{
  "name": "api",
  "version": "1.0.0",
  "description": "Atisbalo Api",
  "private": true,
  "scripts": {
    "dev": "nodemon ./api/server.js",
    "start": "node ./api/server.js",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "migrate:aws": "NODE_ENV=aws sequelize db:migrate",
    "migrate": "sequelize db:migrate",
    "db:init": "sequelize db:drop --env development && sequelize db:create --env development && sequelize db:migrate --env development && nodemon ./api/server.js",
    "db:init:aws": "sequelize db:drop --env aws && sequelize db:create --env aws && sequelize db:migrate --env aws && nodemon ./api/server.js"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "chalk": "^4.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.6",
    "redis": "^3.0.2",
    "sequelize": "^5.21.7"
  },
  "devDependencies": {
    "faker": "^4.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.3",
    "prettier": "2.0.5",
    "sequelize-cli": "^5.5.1",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4"
  }
}
